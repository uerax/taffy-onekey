{
    "routing": {
        "domainStrategy": "AsIs",
        "rules": [
            {
                "type": "field",
                "domain": [
                    "geosite:category-ads-all"
                ],
                "outboundTag": "block"
            }
        ]
    },
    "log": {
        "loglevel": "warning"
    },
    "inbounds": [
        {
            "port": "${port}",
            "protocol": "shadowsocks",
            "settings": {
                "method": "${method}",
                "password": "${password}",
                "level": 0,
                "email": "crayfish@gmail.com",
                "network": "udp,tcp"
            },
            "sniffing": {
                "enabled": true,
                "destOverride": [
                    "http",
                    "tls"
                ]
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom"
        },
        {
            "tag": "block",
            "protocol": "blackhole"
        }
    ]
}