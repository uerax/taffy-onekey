{
    "route": {
    },
    "inbounds": [
        {
            "type": "vless",
            "listen": "::",
            "listen_port": 114514,
            "sniff": true,
            "sniff_override_destination": true,
            "users": [
               {
                  "uuid": "${password}"
               }
            ],
            "tls": {
                "enabled": true,
                "server_name": "${domain}",
                "alpn": [
                    "http/1.1"
                ],
                "min_version": "1.2",
                "max_version": "1.3",
                "acme": {
                    "domain": ["${domain}"],
                    "data_directory": "/etc/sing-box",
                    "default_server_name": "",
                    "email": "singbox@crayfish.com",
                    "provider": "letsencrypt"
                }
            },
            "multiplex": {
                "enabled": true
            },
            "transport": {
                "type": "grpc",
                "service_name": "/${ws_path}"
            }
        }
    ],
    "outbounds": [
        {
            "type": "direct"
        }
    ]
}