{
    "log": {
      "loglevel": "warning"
    },
    "inbounds": [
      {
        "listen": "/dev/shm/Xray-VLESS-gRPC.socket,0666",
        "protocol": "vless",
        "settings": {
          "clients": [
            {
              "id": "${password}"
            }
          ],
          "decryption": "none"
        },
        "streamSettings": {
          "network": "grpc",
          "grpcSettings": {
            "serviceName": "/${ws_path}"
          }
        }
      }
    ],
    "outbounds": [
      {
        "tag": "direct",
        "protocol": "freedom",
        "settings": {}
      },
      {
        "tag": "blocked",
        "protocol": "blackhole",
        "settings": {}
      }
    ],
    "routing": {
      "domainStrategy": "AsIs",
      "rules": [
        {
          "type": "field",
          "ip": [
            "geoip:private"
          ],
          "outboundTag": "blocked"
        }
      ]
    }
  }