{
    "log":{
      "access": "/var/log/xray/access.log",
      "error": "/var/log/xray/error.log",
      "loglevel": "warning"
    },
    "inbounds": [
      {
        "port": 19191,
        "listen": "127.0.0.1",
        "protocol": "vmess",
        "settings": {
          "clients": [{
            "id": "${password}",
            "alterID": 0
          }]
        },
        "streamSettings": {
          "network": "ws",
          "wsSettings": {
            "path": "/${ws_path}"
          }
        }
      }
    ],
    "outbounds": [
      "OutboundsPlaceholder"
    ],
    "routing": {
      "RoutingPlaceholder":0
    }
  }