{
    "log": {
      "loglevel": "warning"
    },
    "inbounds": [
      {
        "listen": "/dev/shm/Xray-Trojan-gRPC.socket,0666",
        "protocol": "trojan",
        "settings": {
          "clients": [
            {
              "password": "${password}"
            }
          ]
        },
        "streamSettings": {
          "network": "grpc",
          "grpcSettings": {
            "serviceName": "${ws_path}"
          }
        }
      }
    ],
    "outbounds": [
      {
        "tag": "direct",
        "protocol": "freedom",
        "settings": {}
      },
      {
        "tag": "blocked",
        "protocol": "blackhole",
        "settings": {}
      }
    ],
    "routing": {
      "domainStrategy": "AsIs",
      "rules": [
        {
          "type": "field",
          "ip": [
            "geoip:private"
          ],
          "outboundTag": "blocked"
        }
      ]
    }
  }