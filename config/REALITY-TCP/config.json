{
    "routing": {
        "domainStrategy": "AsIs",
        "rules": [
        ]
    },
    "log": {
        "loglevel": "info"
    },
    "inbounds": [
        {
            "listen": "0.0.0.0",
            "port": 443,
            "protocol": "vless",
            "key": "${pubicKey}",
            "settings": {
                "clients": [
                    {
                        "id": "${password}",
                        "flow": "xtls-rprx-vision"
                    }
                ],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "tcp",
                "security": "reality",
                "realitySettings": {
                    "show": false,
                    "dest": "www.python.org:443",
                    "serverNames": [
                        "www.python.org"
                    ],
                    "privateKey": "${privateKey}",
                    "shortIds": [
                        "8eb7bab5a41eb27d",
                        ""
                    ]
                }
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom"
        }
    ],
    "policy": {
        "levels": {
            "0": {
                "handshake": 2,
                "connIdle": 120
            }
        }
    }
}