{
    "inbounds": [  
        {
            "listen": "0.0.0.0",
            "port": "${port}",
            "protocol": "socks",
            "tag": "${port}",
            "settings": {
                "auth": "password",
                "accounts": [
                    {
                        "user": "${user}",
                        "pass": "${password}"
                    }
                ],
                "udp": true,
                "ip": "127.0.0.1"
            }
        }
    ]
}