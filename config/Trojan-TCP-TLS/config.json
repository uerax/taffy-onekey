{
    "routing": {
        "domainStrategy": "AsIs",
        "rules": [
            {
                "type": "field",
                "domain": [
                    "geosite:category-ads-all"
                ],
                "outboundTag": "block"
            },
            {
                "type": "field",
                "domain": [
                    "geosite:netflix",
                    "geosite:openai",
                    "geosite:disney",
                    "geosite:spotify"
                ],
                "outboundTag": "WARP"
            }
        ]
    },
    "log": {
        "loglevel": "debug"
    },
    "inbounds": [
        {
            "port": 19191,
            "protocol": "trojan",
            "settings": {
                "clients": [
                    {
                        "password": "${password}",
                        "email": "genshin@gmail.com"
                    }
                ],
                "fallbacks": [
                    {
                        "dest": "/dev/shm/default.sock",
                        "xver": 1
                    },
                    {
                        "alpn": "h2",
                        "dest": "/dev/shm/h2c.sock",
                        "xver": 1
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "tls",
                "tlsSettings": {
                    "alpn": [
                        "http/1.1",
                        "h2"
                    ],
                    "certificates": [
                        {
                            "certificateFile": "${ca_crt}",
                            "keyFile": "${ca_key}",
                            "ocspStapling": 3600
                        }
                    ],
                    "minVersion": "1.2"
                }
            }
        }
    ],
    "outbounds": [
        "OutboundsPlaceholder",
        {
            "tag": "WARP",
            "protocol": "wireguard",
            "settings": {
                "secretKey": "QAlKTfTa7NYlGAmLUBGToRb7YWBvRfjhLDaZKvYzfHU=",
                "address": [
                    "172.16.0.2/32"
                ],
                "peers": [
                    {
                        "publicKey": "bmXOC+F1FxEMF9dyiK2H5/1SUtzH0JuVo51h2wPfgyo=",
                        "endpoint": "engage.cloudflareclient.com:2408"
                    }
                ]
            }
        },
        {
            "tag": "block",
            "protocol": "blackhole"
        }
    ]
}