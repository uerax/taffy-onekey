{
    "route": {
    },
    "inbounds": [
        {
            "type": "trojan",
            "listen": "::",
            "listen_port": 443,
            "users": [
                {
                    "password": "${password}"
                }
            ],
            "tls": {
                "enabled": true,
                "server_name": "${domain}",
                "alpn": ["http/1.1"],
                "min_version": "1.2",
                "max_version": "1.3",
                "acme": {
                  "domain": ["${domain}"],
                  "data_directory": "/etc/sing-box",
                  "default_server_name": "",
                  "email": "singbox@crayfish.com",
                  "provider": "letsencrypt"
                }
            },
            "multiplex": {
                "enabled": false,
                "protocol": "smux",
                "max_streams": 32
            }
        }
    ],
    "outbounds": [
        {
            "type": "direct"
        }
    ]
}